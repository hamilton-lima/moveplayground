{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/next"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "/en"
        },
        {
            "src": "/(.+)",
            "dest": "/$1"
        }
    ]
}